library(easynem)
library(ggplot2)
library(ggpubr)
nem <- read_nem2(tab = nemtab,
tax = nemtax,
meta = nemmeta)
# Calculation of total nematode abundance
nem@meta$all = rowSums(nem@meta[,c(-1,-2)])
p1 = nem |>
calc_compare(.group = Treatments, y = all, method = LSD) |>
nem_plot(type = 2, add = "mean_se")
p1 = p1 + ylab("Nematode abundance\n(Ind. / 100 g dry soil)")
p1
